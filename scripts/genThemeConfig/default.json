{
    "--brand-primary": "#337EFF",
    "--brand-success": "#26BD71",
    "--brand-problem": "#FFB21A",
    "--brand-warning": "#ed6a0c",
    "--brand-error": "#F24957",
    "--brand-disabled": "#ebebeb",
    "--component-text-color": "#333",
    "--component-active-color": "#f2f3f5",
    "--component-active-opacity": "0.7",
    "--component-disabled-opacity": "0.5",
    "--component-background-color": "#f7f8fa",
    "--component-background-color-light": "#fafafa",
    "--component-text-link-color": "#576b95",
    "--font-color": "#333",
    "--font-size-xs": "10px",
    "--font-size-sm": "12px",
    "--font-size-md": "14px",
    "--font-size-lg": "16px",
    "--font-weight-bold": "500",
    "--line-height-xs": "14px",
    "--line-height-sm": "18px",
    "--line-height-md": "20px",
    "--line-height-lg": "22px",
    "--base-font-family": "-apple-system, blinkmacsystemfont, 'Helvetica Neue',\n    helvetica, segoe ui, arial, roboto, 'PingFang SC', 'miui', 'Hiragino Sans GB',\n    'Microsoft Yahei', sans-serif",
    "--price-integer-font-family": "avenir-heavy, pingfang sc, helvetica neue, arial,\n    sans-serif",
    "--linear-layout-background": "rgba(255,255,255,0.5)",
    "--panel-background-color": "#fff",
    "--panel-header-value-color": "#F24957",
    "--panel-footer-padding": "12px 24px",
    "--collapse-item-content-padding": "@padding-sm @padding-md",
    "--collapse-item-content-font-size": "14px",
    "--collapse-item-content-line-height": "1.5",
    "--collapse-item-content-text-color": "#969799",
    "--collapse-item-content-background-color": "#969799",
    "--collapse-item-title-disabled-color": "#ebebeb",
    "--text-font-size": "var(--font-size-base)",
    "--text-font-size-small": "var(--font-size-small)",
    "--text-font-size-large": "calc(var(--font-size-base) * (18 / 14))",
    "--text-color-primary": "var(--brand-primary)",
    "--text-color-secondary": "var(--font-second-color)",
    "--text-color-success": "var(--brand-success)",
    "--text-color-warning": "var(--brand-warning)",
    "--text-color-error": "var(--brand-error)",
    "--text-color-disabled": "var(--brand-disabled-dark)",
    "--text-font-size-huge": "calc(var(--font-size-base) * (22 / 14))",
    "--link-color": "var(--brand-primary)",
    "--link-color-success": "var(--brand-success)",
    "--link-color-warning": "var(--brand-warning)",
    "--link-color-danger": "var(--brand-danger)",
    "--link-color-disabled": "var(--font-disabled-color)",
    "--link-color-hover": "var(--brand-primary-light)",
    "--link-color-success-hover": "var(--brand-success-light)",
    "--link-color-warning-hover": "var(--brand-warning-light)",
    "--link-color-danger-hover": "var(--brand-danger-light)",
    "--link-color-disabled-hover": "var(--brand-disabled-light)",
    "--link-color-error": "var(--brand-error)",
    "--link-color-error-hover": "var(--brand-error-light)",
    "--link-color-light": "#6eadff",
    "--link-color-white": "white"
}
