{
  "extends": "vusion/all",
  "env": {
      "browser": true,
      "node": true
  },
  "rules": {
      "no-alert": [0],
      "function-paren-newline": [0],
      "vue/html-indent": [0],
      "vue/no-unused-vars": [0],
      "linebreak-style": 0,
      "vue/component-definition-name-casing": [2, "kebab-case"],
      "vue/first-attribute-linebreak": [0],
      "vue/no-mutating-props": [0],
      "vue/name-property-casing": 0
  }
}
