{
  "name": "@lcap/mobile-ui",
  "version": "0.12.18",
  "lcapname": "@lcap/mobile-ui",
  "lcapversion": "0.6.3",
  "description": "Mobile UI Components built on Vue",
  "main": "dist-theme/index.js",
  "module22": "es/index.js",
  "style": "dist-theme/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "dist-theme",
    "es",
    "lib",
    "types",
    "vetur"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev:vusion": "cross-env scene=desktop vue-cli-service doc --port 9100",
    "dev": "cross-env scene=desktop vant-cli dev",
    "dev2": "vant-cli dev",
    "lint": "vant-cli lint",
    "test": "vant-cli test",
    "build:vusion": "vue-cli-service library-build --dest dist-theme",
    "build2": "vant-cli build",
    "build": "cross-env scene=desktop vant-cli build",
    "release": "vant-cli release",
    "test:watch": "vant-cli test --watch",
    "release:site": "sh docs/site/release.sh",
    "test:coverage": "open test/coverage/index.html",
    "move-yaml": "node bin/move-yaml",
    "move-docs": "node bin/move-docs",
    "screenshot": "node bin/screenshot",
    "deploy:dev": "node ./scripts/lcap && lcap deploy dist-theme --platform http://defaulttenant.lcap.dev.env.com/ && node bin/deploy-lcp-images --platform=http://defaulttenant.lcap.dev.env.com/",
    "deploy:test": "node ./scripts/lcap && lcap deploy dist-theme --platform http://defaulttenant.lcap.ha.test.com && node bin/deploy-lcp-images --platform=http://defaulttenant.lcap.ha.test.com",
    "deploy:pre": "node ./scripts/lcap && lcap deploy dist-theme --platform http://defaulttenant.lcap.ha.dri.com && node bin/deploy-lcp-images --platform=http://defaulttenant.lcap.ha.dri.com",
    "deploy:online": "node ./scripts/lcap && vusion deploy --framework @lcap/mobile-ui dist-theme && node bin/deploy-screenshots && node bin/deploy-lcp-images --platform=http://netease-template.lcap.163yun.com",
    "prepublishOnly": "npm run build:vusion && git add .",
    "postpublish": "npm run deploy:online"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "vant-cli commit-lint"
    }
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{ts,tsx,js,vue,less}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix",
    "*.{vue,css,less}": "stylelint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:youzan/vant.git"
  },
  "keywords": [
    "ui",
    "vue",
    "frontend",
    "mobile ui",
    "component",
    "components"
  ],
  "author": "youzanfe",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.x",
    "@vant/icons": "^1.7.1",
    "@vant/popperjs": "^1.1.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "cloud-ui.vusion": "0.14.16-beta.3",
    "lodash": "^4.17.21",
    "online-svg-icon-vue2": "^1.2.3",
    "swiper": "^7.4.1",
    "vconsole": "^3.9.3",
    "vue-lazyload": "1.2.3",
    "vue-month-picker": "^1.6.0"
  },
  "peerDependencies": {
    "vue": ">= 2.6.0"
  },
  "devDependencies": {
    "@vant/area-data": "^1.1.1",
    "@vant/cli": "^2.10.3",
    "@vant/eslint-config": "^3.3.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.2.2",
    "@vusion/validator": "^0.3.0",
    "@xgheaven/nos-node-sdk": "^0.2.5",
    "cross-env": "^7.0.3",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "2.1.0",
    "shx": "^0.3.3",
    "vue": "2.6.12",
    "vue-cli-plugin-vusion": "^0.14.0",
    "vue-i18n": "^8.26.0",
    "vue-router": "^3.5.2",
    "vue-template-compiler": "2.6.12",
    "vusion": "^0.12.18",
    "vusion-api": "^0.7.36",
    "yaml": "^1.10.2"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ],
  "vetur": {
    "tags": "vetur/tags.json",
    "attributes": "vetur/attributes.json"
  },
  "web-types": "vetur/web-types.json",
  "eslintConfig": {
    "root": true,
    "extends": [
      "@vant"
    ]
  },
  "stylelint": {
    "extends": [
      "@vant/stylelint-config"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "proseWrap": "never"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
